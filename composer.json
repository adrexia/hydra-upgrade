{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "^3",
		"silverstripe/framework": "^3",
		"mateusz/frontend": "dev-master",
		"silverstripe-australia/memberprofiles": "1.1.1b",
		"unclecheese/betterbuttons": "1.3.14",
		"silverstripe/dashboard": "1.1.2",
		"adrexia/gamesevent": "dev-master",
		"silverstripe/userforms": "3.0.2",
		"silverstripe/html5": "1.0.3",
		"silverstripe/iframe": "1.0.7",
		"silverstripe/mimevalidator": "1.0.0",
		"silverstripe/multivaluefield": "2.0.3",
		"silverstripe/queuedjobs": "2.2.1",
		"silverstripe/restfulserver": "1.0.2",
		"silverstripe/securityreport": "1.0.1",
		"silverstripe/selectupload": "1.0.1",
		"silverstripe/spellcheck": "1.0.0",
		"silverstripe/standardsediting": "1.1.6",
		"jonom/silverstripe-betternavigator": "3.0.2",
		"adrexia/dashboardmods": "dev-master",
		"sheadawson/silverstripe-linkable": "1.2.*",
		"unclecheese/display-logic": "^1.7"
	},
	"config": {
		"process-timeout": 600,
		"allow-plugins": {
			"composer/installers": true
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/adrexia/silverstripe-memberprofiles.git"
		}
	],
	"minimum-stability": "dev"
}
